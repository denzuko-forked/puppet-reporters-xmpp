{
  "name": "denzuko-reporters-xmpp",
  "version": "0.1.0",
  "author": "Denzuko",
  "summary": "A Puppet report processor that announces failed runs to a Jabber-XMPP user",
  "license": "Apache-2.0",
  "source": "https://github.com/denzuko-forked/puppet-xmpp",
  "project_page": "https://github.com/denzuko-forked/puppet-xmpp/wiki",
  "issues_url": "https://github.com/denzuko-forked/puppet-xmpp/issues/",
  "dependencies": [
    { "name":"puppetlabs/stdlib","version_requirement":">= 4.13.1 < 5.0.0" }
  ],
  "data_provider": "hiera",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "16.04",
        "18.04"
       ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.5.0 < 5.0.0"
    }
  ],
  "description": "A Puppet report processor that announces failed runs to a Jabber-XMPP user."
}
